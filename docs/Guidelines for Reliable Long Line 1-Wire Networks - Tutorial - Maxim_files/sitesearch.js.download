templates.capitalize = function (text) {
    var returntext = "";
    if (typeof text !=='undefined'){
	var arrayOfText = text.split('/');
    for (var i = 0; i < arrayOfText.length; i++) {
        if (i>0) {
            returntext +="<br/>"
        }
        if (arrayOfText[i].indexOf('mvp')==-1) {
			returntext+= "" + arrayOfText[i].capitalize();
        }
    }
    }
    return returntext;
}

