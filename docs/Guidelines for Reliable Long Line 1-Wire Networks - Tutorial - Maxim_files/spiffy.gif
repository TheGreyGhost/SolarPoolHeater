<script>
    function setCSSForDesktopFooter()
    {
        $('.footer-container').addClass('nobr');
        $('.stick').attr('style','display:inline !important');
        if($('#footer_bottom_anchor').length>0)
        {
			$('#footer_bottom_anchor').after($('#footer'));
        }
    }
    function setCSSForMobileFooter()
    {
        $('.footer-container').removeClass('nobr');
        $('.stick').removeAttr("style")
        if($('#footer_rightbar_anchor').length>0)
        {
			$('#footer_rightbar_anchor').after($('#footer'));
        }
    }
    $(function(){
        if($(window).width() <SMALL_SCREEN_WIDTH){
            setCSSForMobileFooter();
        }
        $(window).bind("resize",function(){
            if($(this).width() <SMALL_SCREEN_WIDTH){
                setCSSForMobileFooter();
            }
            else{
                setCSSForDesktopFooter();
            }
        });
        if('zh'==getUrlLanguage()){
            $('#wechat_anchor').popover({
                  html: true,
                  placement: 'top',
                  trigger:'hover',
                  content: function () {
                    return '<img src="/etc/designs/maximintegrated/_images/icons/qrcode.png" />';
                  }
                });
            $('#wechat_anchor').popover("show");
            $('#wechat_anchor').popover("hide");

            setTimeout(function (){
                $('.addthis_button_sinaweibo_follow').attr("href", "http://www.weibo.com/maximintegrated");
            }, 500);
        }
    });
</script>




