function updateQuantity(quantityId,partListId){
	    //console.log('quantityId : '+quantityId);
	    var qty = $('#'+partListId+' .quantity').val();
        qty = qty.trim();
		var orderInMult = $("#mult-"+partListId).text(); 
	    var orderInMin = $("#min-"+partListId).text(); 
		console.log('value : '+qty+' : '+orderInMult+' : '+orderInMin);
		if(qty != "" && orderInMult != "" && orderInMin != "" ){
			if(parseInt(qty) < parseInt(orderInMin)){
				$('#'+partListId+' .quantity').val(orderInMin);
	            $('#'+quantityId+'-error').hide();
	            $('#'+partListId+'-label').show();
			} else if(qty % orderInMult == 0){
				$('#'+partListId+'-label').hide();
	            $('#quantity-label').hide();
			} else {
				var modvalue = parseInt(qty) % parseInt(orderInMult);
                var q4 = parseInt(orderInMult)-parseInt(modvalue);
                var nearestNum = parseInt(qty) + parseInt(q4);
                //var nearestNum = (Math.round(qty / orderInMult)+1) * orderInMult;
	            $('#'+partListId+' .quantity').val(nearestNum);
	            $('#'+quantityId+'-error').hide();
	            $('#'+partListId+'-label').show();
			}
  
		} else{
			//bootbox.alert('Please enter a number');
		}
	
}

function searchProductCatalog(partNumber, partListId) {
    var hasQuote = false;
    $(".addPart").addClass("disabled");
    //$('#partNo1').val(partNumber);
	//alert('partListId: ' + partListId);
    if(partNumber.length < 4) {
        $(".addPart").addClass("disabled");        
        $("#min-"+partListId).text("");
        $("#mult-"+partListId).text("");
        return false;
    }
    else{
        var link = "/en/appservice/productservice/_jcr_content.productsearch.json?partNo=" + encodeURIComponent(partNumber);
	$.ajax(
			{
				url : link,
				type: "GET",
				data : "JSON",
				success:function(data) 
				{
                    if (data.status && data.size > 0) {                        
                    	orderableParts = data.products;                                                
                        $.each(orderableParts, function() {
                            if(this.canQuote == "false")
                                {
                                    hasQuote = true;
                                    return null;
                                }                            
                        });
                        
                        if(hasQuote) {
                            var temp = partListId.replace("partList", "");
							if($('#requestQuote').css('display') != "none"){
								bootbox.alert('Please enter a valid quotable part number'); 
							}
                              
                            $(".addPart").addClass("disabled");
                            $("#min-"+partListId).text("");
                            $("#mult-"+partListId).text("");
                            $('#partNo' + temp).val("");
                            //$("#min-"+partListId).addClass("datapartcanQuote");
                        }
                        else{
                            //$("#quoteFirstFormSubmit").removeClass("disabled");    
                            $(".addPart").removeClass("disabled");
                            $("#min-"+partListId).text(orderableParts[0].min);
                            $("#mult-"+partListId).text(orderableParts[0].mult);                     
                            $("#"+partListId+" input.quantity").val('');
                            $("#"+partListId+" input.quantity").attr("placeholder", "Enter quantity in multiples of" + orderableParts[0].mult);
                            checkRestrictedParts(orderableParts);
                        }                            
                    }
                    else {
						if($('#requestQuote').css('display') != "none"){
								bootbox.alert('Please enter a valid quotable part number'); 
							}
                        //$("#quoteFirstFormSubmit").addClass("disabled");
                        $(".addPart").addClass("disabled");
                        $("#min-"+partListId).text("");
                        $("#mult-"+partListId).text("");
                        $('#partNo' + partListId).text("");
                    }
				},
			});
    }    
}

