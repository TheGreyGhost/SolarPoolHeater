<script type="text/javascript">
    function maximLoginFormValidation(formName, redirectTo) {
        if (CQ_Analytics) {
            var u = document.forms[formName]['j_username'].value;
            var p = document.forms[formName]['j_password'].value;
            if (CQ_Analytics.Sitecatalyst) {
                CQ_Analytics.record({
                    event: "loginAttempt",
                    values: {
                        username:u,
                        loginPage:"/content/maximintegrated/en/site-map.html",
                        destinationPage:redirectTo
                    },
                    componentPath: 'maximintegrated/components/page/basepage'
                });
                if (CQ_Analytics.ClickstreamcloudUI && CQ_Analytics.ClickstreamcloudUI.isVisible()) {
                    return false;
                }
            }
            
                if (CQ_Analytics && CQ_Analytics.CCM) {
                    login.status=false;
                    if(CQ_Analytics.PersonalInfoStoreMgr)
                        CQ_Analytics.PersonalInfoStoreMgr.setProperty("isLoggedIn","false");
                    if(CQ_Analytics.ProfileDataMgr)
                    	CQ_Analytics.ProfileDataMgr.clear();
                    CQ_Analytics.CCM.reset();
                }
            	CQ.shared.HTTP.clearCookie("CommercePersistence", "/");
                var error="";
                if (u=="") {
                    error += Granite.I18n.get("emailempty")+"\n";
                }
                if (p=="") {
                    error += Granite.I18n.get("pwdempty")+"\n";
                }
                if(error!="")
                {
                    alert(error);
                    return false;
                }
            return true;    
        
        }
